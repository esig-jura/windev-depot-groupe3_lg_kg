        enabled : false
  -
    name : bt_ajouter_categorie
    identifier : 0x23032b69019222ea
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         
         //La fonction HReset initialise toutes les variables des rubriques d'un fichier de données avec leurs valeurs par défaut
         HReset(TB_CATEGORIE)
         
         //ouvrir la fenêtre de saisie
         Open(FE_Categories_saisie)
         
         //quand on fait un close sur la fenêtre de saisie on revient ici
         //on réaffecte la source de données (table Recette) à la table IHM
         //ainsi toutes les données sont rechargées
         //on positionne le curseur sur le dernier enregistrement
         TableDisplay(Table_CATEGORIE, taCurrentRecord)
        type : 18
