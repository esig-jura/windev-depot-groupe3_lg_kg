  -
    name : bt_supprimer_categorie
    identifier : 0x23032b2201840b9e
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         //Affiche un message à l'utilisateur
         if YesNo(No, "Voulez-vous vraiment supprimer cet enregistrement ?") then
         	HDelete(TB_CATEGORIE)
         	//Mettre à jour la table IHM avec les données de la BD
         	Table_CATEGORIE.BrowsedFile = "TB_CATEGORIE"
         else
         	Info("Suppression abandonnée")
         end
         
         //quand on fait un close sur la fenêtre de saisie on revient ici
         //on réaffecte la source de données (table Recette) à la table IHM
         //ainsi toutes les données sont rechargées
         //on positionne le curseur sur le dernier enregistrement
         TableDisplay(Table_CATEGORIE, taCurrentSelection)
        type : 18
      -
        type : 161
        enabled : false
  -
    name : bt_modifier_categorie
    identifier : 0x23032b57018bddd2
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         //Ouvre la fenêtre ci-dessous
         Open(FE_Categories_saisie)
         
         //quand on fait un close sur la fenêtre de saisie on revient ici
         //on réaffecte la source de données (table Recette) à la table IHM
         //ainsi toutes les données sont rechargées
         //on positionne le curseur sur le dernier enregistrement
         TableDisplay(Table_CATEGORIE, taCurrentSelection)
        type : 18
      -
