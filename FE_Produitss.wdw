  -
    name : BT_Supprimer_produits
    identifier : 0x23049ab70536bf9b
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         //Affiche un message à l'utilisateur
         IF YesNo(No, "Voulez-vous vraiment supprimer cet enregistrement ?") THEN
         	HDelete(TB_TYPE_SKI_SNOW)
         	//Mettre à jour la table IHM avec les données de la BD
         	
         	Table_produits.BrowsedFile = "TB_TYPE_SKI_SNOW"
         ELSE
         	Info("Suppression abandonnée")
         END
         
         //quand on fait un close sur la fenêtre de saisie on revient ici
         //on réaffecte la source de données (table Recette) à la table IHM
         //ainsi toutes les données sont rechargées
         //on positionne le curseur sur le dernier enregistrement
         TableDisplay(Table_produits, taCurrentRecord)
        type : 18
      -
        type : 161
        enabled : false
  -
    name : BT_Modifier_produits
    identifier : 0x23049ab70537bf9b
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         
         //Deuxième champ permet d'envoyer le numéro en "fonction" à l'autre fenêtre
         open(FE_produits_saisie,Table_produits.ID_prod)
         
         
         //quand on fait un close sur la fenêtre de saisie on revient ici
         //on réaffecte la source de données (table Recette) à la table IHM
         //ainsi toutes les données sont rechargées
         //on positionne le curseur sur le dernier enregistrement
         TableDisplay(Table_produits, taCurrentRecord)
        type : 18
      -
        type : 161
        enabled : false
  -
    name : BT_Ajouter_produits
    identifier : 0x23049ab70538bf9b
    internal_properties : 
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         
         //La fonction HReset initialise toutes les variables des rubriques d'un fichier de données avec leurs valeurs par défaut
         HReset(TB_PRODUIT)
         
         //ouvrir la fenêtre de saisie avec paramètre
         Open(FE_produits_saisie, 0)
         
         //quand on fait un close sur la fenêtre de saisi on revient ici
         //on reaffecte la source de données (table Recette) à la table IHM
         //ainsi toutes les données sont rechargées
         //on positionne le curseur sur le dernier enregistremement
         TableDisplay(Table_produits, taCurrentRecord)
         
         
        type : 18
      -
        type : 161
        enabled : false
